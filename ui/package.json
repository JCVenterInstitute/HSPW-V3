{
  "name": "hsp-ui",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "local:frontend": "npm run dev",
    "local:backend": "npm run dev --prefix src/server",
    "local": "concurrently \"npm run local:frontend\" \"npm run local:backend\"",
    "start:local": "NODE_TLS_REJECT_UNAUTHORIZED=0 DEPLOY_ENV=LOCAL nodemon -r dotenv/config server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy:stackrequest:core:dev": "sam deploy --config-env stack-request-core-dev",
    "deploy:stackrequest:core:prod": "sam deploy --config-env stack-request-core-prod",
    "upload:dev": "aws --region us-east-2 s3 sync --delete --acl public-read build/ s3://static-content-hsp-dev/hsp-ui",
    "upload:prod": "aws --region us-east-2 s3 sync --delete --acl public-read build/ s3://static-content-hsp-prod/hsp-ui/build",
    "sam:build:core": "rm -rf .aws-sam && sam build --config-env core",
    "deploy:app:core:dev": "sam deploy --config-env app-core-dev",
    "deploy:app:core:prod": "sam deploy --config-env app-core-prod",
    "complete:app:core:dev": "npm run build:dev && npm run upload:dev && npm run sam:build:core && npm run deploy:app:core:dev",
    "complete:app:core:prod": "npm run build:prod && npm run upload:prod && npm run sam:build:core && npm run deploy:app:core:prod"
  },
  "dependencies": {
    "@aws-sdk/credential-provider-node": "^3.451.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.14.18",
    "@mui/x-date-pickers": "^6.18.1",
    "@opensearch-project/opensearch": "^2.4.0",
    "@testing-library/react": "^13.4.0",
    "@turinggroup/serverless-express-custom-domain-middleware": "^1.1.0",
    "@vendia/serverless-express": "^4.10.1",
    "ag-grid-community": "^28.2.1",
    "ag-grid-react": "^28.2.1",
    "aws-opensearch-connector": "^1.1.0",
    "axios": "^1.3.4",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "d3": "^5.16.0",
    "dayjs": "^1.11.10",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "html-react-parser": "^5.0.6",
    "material-ui-popup-state": "^5.0.4",
    "papaparse": "^5.4.1",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primereact": "^8.7.3",
    "ProtVista": "^2.0.13",
    "protvista-uniprot": "^2.10.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-donut-chart": "^1.3.3",
    "react-fontawesome": "^1.7.1",
    "react-ga4": "^2.1.0",
    "react-google-charts": "^4.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.7.1",
    "react-material-ui-carousel": "^3.4.2",
    "react-msaview": "^2.1.5",
    "react-router-dom": "^6.6.1",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.3",
    "react-tabs": "^6.0.2",
    "react-twitter-widgets": "^1.11.0",
    "react-xml-parser": "^1.1.8",
    "resize-observer-polyfill": "^1.5.1",
    "serve": "^14.1.2",
    "styled-components": "^5.3.11",
    "svg-sprite-loader": "^6.0.11",
    "sweetalert2": "^11.7.32",
    "web-vitals": "^2.1.4",
    "xml2js": "^0.4.23"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-react-app": "^7.0.1"
  },
